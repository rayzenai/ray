{
    "name": "rayzenai/ray",
    "description": "A lightweight Ray-like debugging tool for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "RayzenAI",
            "email": "timsinakiran@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "RayzenAI\\Ray\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "RayzenAI\\Ray\\RayServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
